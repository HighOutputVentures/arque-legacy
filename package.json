{
  "name": "arque",
  "version": "0.1.4",
  "description": "RabbitMQ based microservice framework.",
  "main": "build/index.js",
  "scripts": {
    "test": "ava",
    "clean": "rm -rf build",
    "build": "npm run clean && babel src --out-dir build"
  },
  "keywords": [
    "queue",
    "rabbitmq",
    "microservice",
    "worker"
  ],
  "author": "Roger Madjos",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/rogermadjos/arque.git"
  },
  "dependencies": {
    "amqplib": "^0.5.1",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "ava": "^0.18.2",
    "babel-cli": "^6.23.0",
    "babel-plugin-transform-async-to-generator": "^6.22.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.23.0",
    "babel-register": "^6.23.0",
    "eslint": "^3.16.1",
    "eslint-config-standard": "^7.0.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^2.1.1",
    "rand-token": "^0.3.0"
  },
  "ava": {
    "files": [
      "test/*.js",
      "!test/helpers.js"
    ],
    "timeout": "10s",
    "concurrency": 5,
    "failFast": true,
    "powerAssert": false,
    "require": [
      "babel-register"
    ],
    "babel": "inherit",
    "verbose": true
  },
  "engines": {
    "node": ">= 6.10.0"
  }
}
