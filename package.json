{
  "name": "arque",
  "version": "0.4.0",
  "description": "RabbitMQ based microservice framework.",
  "main": "dist/index.js",
  "scripts": {
    "test": "ava",
    "clean": "rm -rf dist",
    "build": "npm run clean && babel src --out-dir dist",
    "flow": "flow check"
  },
  "keywords": [
    "queue",
    "rabbitmq",
    "microservice",
    "worker"
  ],
  "author": "Roger Madjos",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/rogermadjos/arque.git"
  },
  "dependencies": {
    "amqplib": "^0.5.1",
    "lodash": "^4.17.4",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "ava": "^0.18.2",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-register": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^7.1.0",
    "eslint-plugin-flowtype": "^2.34.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^2.3.1",
    "flow-bin": "^0.48.0"
  },
  "ava": {
    "files": [
      "test/*.js",
      "!test/helpers.js"
    ],
    "timeout": "10s",
    "concurrency": 5,
    "failFast": true,
    "powerAssert": false,
    "require": [
      "babel-register"
    ],
    "babel": "inherit",
    "verbose": true
  },
  "engines": {
    "node": ">= 6.10.0"
  }
}
